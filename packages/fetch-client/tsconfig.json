{
    "compilerOptions": {
        "target": "es5",
        "strict": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "module": "commonjs",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "dist"
    },
    "exclude": ["node_modules", "**/*.test.ts"]
}
